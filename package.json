{
  "name": "replit-ai-platform",
  "version": "1.0.0",
  "description": "منصة برمجة متكاملة تشبه Replit مع ذكاء اصطناعي - Replit AI Platform",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "code-editor",
    "replit",
    "monaco-editor",
    "collaboration",
    "gemini",
    "nodejs",
    "react"
  ],
  "author": "Replit AI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^11.11.1",
    "openai": "^4.20.1",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1",
    "socket.io": "^4.7.4",
    "ws": "^8.14.2",
    "yjs": "^13.6.8",
    "y-websocket": "^1.5.0",
    "y-monaco": "^0.0.3",
    "jszip": "^3.10.1",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "node-fetch": "^3.3.2",
    "axios": "^1.6.2",
    "child_process": "^1.0.2",
    "vm2": "^3.9.19",
    "dockerode": "^4.0.2",
    "express-ws": "^5.0.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}